{
   "domainStrategy": "IPIfNonMatch",
   "domainMatcher": "mph",
   "rules": [
      {
        "type": "field",
        "domain": [
          "domain:bochk.com",
          "domain:hk.centanet.com",
          "domain:hk.centadata.com",
          "domain:alibabacloud.com",
          "domain:aliyun.com"
        ],
        "outboundTag": "direct"
      },
      {
        "type": "field",
        "domain": [
          "geosite:cn",
          "geosite:epochmediagroup",
          "regexp:(.*.||)(dafahao|minghui|dongtaiwang|epochtimes|ntdtv|falundafa|wujieliulan|zhengjian).(org|com|net)"
        ],
        "outboundTag": "blackhole"
      },
      {
        "type": "field",
        "ip": [
          "geoip:cn",
          "geoip:private"
        ],
        "outboundTag": "blackhole"
      },
      {
        "type": "field",
        "protocol": [
          "bittorrent"
        ],
        "outboundTag": "blackhole"
      },
      {
        "type": "field",
        "ip": [
          "127.0.0.1/32",
          "10.0.0.0/8",
          "fc00::/7",
          "fe80::/10",
          "172.16.0.0/12"
        ],
        "outboundTag": "blackhole"
      }
   ]
}
